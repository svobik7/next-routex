{
  "name": "next-roots-example",
  "version": "1.0.0",
  "description": "Example of using next-roots package",
  "author": "Jirka Svoboda",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lintel": "node ../dist/bin/cli.js",
    "dev": "yarn lintel build && next dev",
    "build": "yarn lintel build && next build",
    "start": "next start",
    "analyze": "yarn lintel build && cross-env ANALYZE=true next build",
    "analyze:server": "yarn lintel build && cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "yarn lintel build && cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "next": "^13.1.5",
    "next-lintel": "link:../",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.5",
    "@types/react": "^18.0.27",
    "cross-env": "^7.0.2"
  }
}
